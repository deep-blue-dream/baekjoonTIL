package Baekjoon;

import java.util.Scanner;

public class Quiz1000 {

	public static void main(String[] args) {
		//키보드로 값을 입력하기 위한 Scanner 타입 선택, Scanner은 java.util로 import해야 한다.
		// Scanner의 인스턴스 명은 sc,scan,sc를 주로 쓴다고 한다.
		
    Scanner sc = new Scanner(System.in);
		//입력받을 값을 저장할 변수 a,b 선언
		
    int a, b;
		//입력받는대로 변수에 초기화한다. 입력받을 변수의 자료형에 맞추어 nextInt(), nextDouble, nextBoolean을 사용한다.
		//단, String은 입력방법에 sc.next();와 sc.nextLine(); 두 종료가 있는데, 입력받는 방식이 다르다.
		
		//sc.next(); : 문자열 입력시 공백 전까지만 받는다
		//sc.nextLine(); : 문자열 입력시 Enter 전까지만 받는다.
		
    a = sc.nextInt();
		b = sc.nextInt();
		System.out.println(a+b);
		
		sc.close();
	}

}
